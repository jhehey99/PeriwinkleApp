<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:layout_marginTop="30dp"
		android:layout_marginHorizontal="30dp"
        android:orientation="horizontal">
	    <TextView
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.6"
		    android:text="Date of Response: "
		    android:textColor="#4689C8"
		    android:textSize="20dp"/>
         <TextView
            android:id="@+id/score_date"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.4"
		    android:text="00/00/0000"
		    android:textColor="#4689C8"
            android:background="#c8e8ed"
            android:textAlignment="center"
		    android:textSize="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:layout_marginTop="20dp"
		android:layout_marginHorizontal="30dp"
        android:orientation="horizontal">
	    <TextView
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.6"
		    android:text="Height: "
		    android:textColor="#4689C8"
		    android:textSize="20dp"/>
         <TextView
            android:id="@+id/score_height"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.4"
		    android:text="0cm "
		    android:textColor="#4689C8"
            android:background="#c8e8ed"
            android:textAlignment="center"
		    android:textSize="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:layout_marginTop="20dp"
		android:layout_marginHorizontal="30dp"
        android:orientation="horizontal">
	    <TextView
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.6"
		    android:text="Weight: "
		    android:textColor="#4689C8"
		    android:textSize="20dp"/>
         <TextView
            android:id="@+id/score_weight"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.4"
		    android:text="0kg "
		    android:textColor="#4689C8"
            android:background="#c8e8ed"
            android:textAlignment="center"
		    android:textSize="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:layout_marginTop="20dp"
		android:layout_marginHorizontal="30dp"
        android:orientation="horizontal">
	    <TextView
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.6"
		    android:text="BMI: "
		    android:textColor="#4689C8"
		    android:textSize="20dp"/>
         <TextView
            android:id="@+id/score_bmi"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.4"
		    android:text="0"
		    android:textColor="#4689C8"
            android:background="#c8e8ed"
            android:textAlignment="center"
		    android:textSize="20dp"/>
    </LinearLayout>


    <TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="30dp"
		android:layout_marginHorizontal="30dp"
		android:text="MBES-21 Response Average Scores"
		android:textColor="#4689C8"
		android:textSize="20dp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:layout_marginTop="20dp"
		android:layout_marginHorizontal="30dp"
        android:orientation="horizontal">
	    <TextView
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.6"
		    android:text="Binge Eating: "
		    android:textColor="#4689C8"
		    android:textSize="20dp"/>
         <TextView
            android:id="@+id/score_binge"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.4"
		    android:text="00.00"
		    android:textColor="#4689C8"
            android:background="#c8e8ed"
            android:textAlignment="center"
		    android:textSize="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:layout_marginTop="20dp"
		android:layout_marginHorizontal="30dp"
        android:orientation="horizontal">
	    <TextView
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.6"
		    android:text="Bulimia Nervosa: "
		    android:textColor="#4689C8"
		    android:textSize="20dp"/>
         <TextView
            android:id="@+id/score_bulimia"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.4"
		    android:text="00.00"
            android:background="#c8e8ed"
		    android:textColor="#4689C8"
            android:textAlignment="center"
		    android:textSize="20dp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
		android:layout_marginTop="20dp"
		android:layout_marginHorizontal="30dp"
        android:orientation="horizontal">
	    <TextView
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.6"
		    android:text="Anorexia Nervosa: "
		    android:textColor="#4689C8"
		    android:textSize="20dp"/>
         <TextView
            android:id="@+id/score_anorexia"
		    android:layout_width="0dp"
		    android:layout_height="wrap_content"
            android:layout_weight="0.4"
		    android:text="00.00"
		    android:textColor="#4689C8"
            android:background="#c8e8ed"
            android:textAlignment="center"
		    android:textSize="20dp"/>
    </LinearLayout>

</LinearLayout>
